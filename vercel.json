{
    "buildCommand": "expo export -p web",
    "outputDirectory": "dist",
    "devCommand": "expo",
    "cleanUrls": true,
    "framework": null,
    "headers": [
        {
            "source": "/.well-known/apple-app-site-association",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/json"
                }
            ]
        },
        {
            "source": "/.well-known/assetlinks.json",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/json"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/"
        }
    ]
}